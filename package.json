{
  "name": "mknew",
  "version": "1.0.2",
  "description": "Stupidly simple scaffolding.",
  "author": "Chris Ackerman",
  "license": "ISC",
  "keywords": [
    "scaffold",
    "template",
    "generator",
    "generate",
    "boilerplate",
    "directory",
    "setup",
    "create",
    "init",
    "cli",
    "dev",
    "development",
    "project",
    "package",
    "module",
    "bundle",
    "tool",
    "yeoman"
  ],
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "mknew": "lib/bin.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "del-cli lib && tsc && chmodx --package",
    "prepack": "npm test && npm run build",
    "start": "npm --silent run build && node lib/bin.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cross-spawn": "^7.0.3",
    "istextorbinary": "^6.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "chmodx": "^1.0.0",
    "del-cli": "^4.0.1",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/mknew.git"
